<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.creditplus.p2p.dao.CustomerInfoDao">
    
    <insert id="insert" parameterType="java.util.Map">
    	insert into customer_info_t(id,user_id,thnic_v,registered_place_v,address_phone,address_phone_v,current_city_v,current_address_v,id_num_name_v,id_num_v,mobile_place_v,company_name_v,work_tel_v,work_tel_place_v,work_name_v,work_position_v,income_v,income_name_v,seasame_score_v,tencent_credit_v,certificate_type_v,certificate_name_v,profession_grade_v,mobile_name_v,mobile_online_time_v,profession_code,degree_name_v,school_name_v,highest_degree_v,created_by,created_date,last_updated_by,last_updated_date)values(#{id},#{user_id},#{thnic_v},#{registered_place_v},#{address_phone},#{address_phone_v},#{current_city_v},#{current_address_v},#{id_num_name_v},#{id_num_v},#{mobile_place_v},#{company_name_v},#{work_tel_v},#{work_tel_place_v},#{work_name_v},#{work_position_v},#{income_v},#{income_name_v},#{seasame_score_v},#{tencent_credit_v},#{certificate_type_v},#{certificate_name_v},#{profession_grade_v},#{mobile_name_v},#{mobile_online_time_v},#{profession_code},#{degree_name_v},#{school_name_v},#{highest_degree_v},#{last_updated_by},sysdate(),#{last_updated_by},sysdate())
    </insert>
    
    <delete id="deleteByUserId" parameterType="Integer">
    	delete from customer_info_t  where user_id = #{user_id}
    </delete>
    
</mapper>