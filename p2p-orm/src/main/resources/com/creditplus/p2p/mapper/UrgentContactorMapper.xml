<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.creditplus.p2p.dao.UrgentContactorDao">
    
	<select id="getListByUserId" parameterType="Integer" resultType="java.util.Map">
		select t.id,t.user_id,t.name,t.relation,t.mobile,t.mobile_address,t.created_by,t.created_date,t.last_updated_by,t.last_updated_date from urgent_contactor_p t where t.user_id = #{user_id}
	</select>    
	
	<insert id="insertBatch" parameterType="java.util.Map">
		insert into urgent_contactor_p(id,user_id,name,relation,mobile,mobile_address,created_by,created_date,last_updated_by,last_updated_date)values(#{id},#{user_id},#{name},#{relation},#{mobile},#{mobile_address},#{created_by},#{created_date},#{last_updated_by},#{last_updated_date})
	</insert>
</mapper>